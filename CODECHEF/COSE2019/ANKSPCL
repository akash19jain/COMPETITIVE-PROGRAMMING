#include<stdio.h>
#include<math.h>
#include<string.h>
#include<stdlib.h>
#include<stdbool.h>
#define mod 1000000007

int main()
{
    long long t;
    scanf("%lld\n",&t);
    while(t--)
    {
        char ch;
        scanf("%c\n", &ch);
        printf("%d\n",ch-97);
        
        
    }
}
